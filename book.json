{
    "plugins": [ "-search", "-lunr", "scripts", "collapsible-menu", "page-toc", "include-codeblock", "include-csv" ],
    "pluginsConfig": {
        "scripts": {
            "files": [
                "node_modules/encoding-japanese/encoding.min.js",
                "scripts/script.js"
            ]
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "after-first",
            "showByDefault": true
        }
    },
    "styles": {
        "website": "styles/style.css"
    }
}
